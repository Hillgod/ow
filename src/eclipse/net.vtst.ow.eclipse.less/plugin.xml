<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

  <!-- Declare the content type -->

  <extension point="org.eclipse.core.contenttype.contentTypes">
    <content-type base-type="org.eclipse.core.runtime.text"
                  file-extensions="%less_file_extension"
                  id="net.vtst.ow.eclipse.less"
                  name="%less_content_type"
                  priority="normal">
    </content-type>
  </extension>

  <!-- Project nature -->
  
  <extension point="org.eclipse.core.resources.natures"
    id="nature"
    name="%less_nature">
    <runtime>
      <run class="net.vtst.eclipse.easyxtext.nature.NullProjectNature"/>
    </runtime>
    <requires-nature id="org.eclipse.xtext.ui.shared.xtextNature"/>
  </extension>

  <!-- This is the part generated by XText. -->

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http://www.vtst.net/ow/eclipse/less/Less" 
       class = "net.vtst.ow.eclipse.less.less.LessPackage"
       genModel = "net/vtst/ow/eclipse/less/Less.genmodel" /> 	
  </extension>

</plugin>
